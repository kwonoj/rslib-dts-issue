{
  "compilerOptions": {
    "incremental": true,
    "composite": true,
    "sourceMap": false,
    "declarationMap": false,
    "emitDeclarationOnly": true,
    "jsx": "react-jsx",
    "outDir": "${configDir}/dist/tsc",
    "tsBuildInfoFile": "${configDir}/dist/tsc/tsconfig.tsbuildinfo",
    "module": "esnext",
    "moduleResolution": "Bundler",
    "target": "ES2022",
    "lib": ["ES2022", "DOM"],
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true
  }
}
